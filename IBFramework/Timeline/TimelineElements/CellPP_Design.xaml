<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:IBFramework.Timeline.TimelineElements"
                    xmlns:ibgui="clr-namespace:IBGUI;assembly=IBGUI">

    <Style TargetType="{x:Type local:CellPP}">
        <Setter Property="Foreground" Value="{DynamicResource IBForeGroundBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:CellPP}">
                    <Grid MinWidth="150">
                        <StackPanel Width="100" Margin="10,0,0,0" HorizontalAlignment="Left">
                            <Label Content="ID" HorizontalAlignment="Left" Height="22" Padding="0" Background="#FFC18989"/>
                        </StackPanel>
                        <StackPanel Margin="110,0,0,0" Background="White">
                            <Label Content="{Binding ID}" Height="22" Padding="0" Background="#FF22A428"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>